	call(CIRCLE4);
	call(CIRCLE4);
	call(CIRCLE4);
	set_val(PS4_SQUARE, 100);
	wait(100);
	wait(800);
	set_val(PS4_CIRCLE, 100);
	wait(100);
	wait(600);
	combo_stop(PICNIC);
	}
combo EGGTIMER {
	wait(210000); 		  //waits 3.5 min. in milliseconds
	set_val(PS4_RY, 50);  //Moves in-game camera to prevent switch from sleep while away.
	wait(100);
	wait(200);
	set_val(PS4_RY, -50); //Moves in-game camera to prevent switch from sleep while away.
	wait(100);
	wait(200);
	wait(210000);	      //waits 3.5 min. in milliseconds
}
combo SPRINT {
	set_val(PS4_L3, 100);
	wait(400);
}
combo HATCH {
    if(combo_running(HATCH)) combo_run(SPRINT); combo_run(TURBO);
	set_val(PS4_LX, -70);
	set_val(PS4_LY, -78);
	set_val(PS4_RX, -65);
	set_val(PS4_RY, +89);
	wait(120000);
	}
