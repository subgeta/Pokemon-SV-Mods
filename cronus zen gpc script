	wait(800);
	set_val(PS4_CIRCLE, 100);
	wait(100);
	wait(800);
	}
combo CIRCLE4 {
	set_val(PS4_CIRCLE, 100);
	wait(100);
	wait(800);
	set_val(PS4_CIRCLE, 100);
	wait(100);
	wait(800);
	set_val(PS4_CIRCLE, 100);
	wait(100);
	wait(800);
	set_val(PS4_CIRCLE, 100);
	wait(100);
	wait(800);
	}
combo RELEASE {
	wait(750);
	colorled(1,0,0,0);
	set_val(PS4_CIRCLE, 100);
	wait(100);
	wait(500);
	set_val(PS4_UP, 100);
	wait(100);
	wait(400);
	set_val(PS4_UP, 100);
	wait(100);
	wait(400);
	set_val(PS4_CIRCLE, 100);
	wait(100);
	wait(600);
	set_val(PS4_UP, 100);
	wait(100);
	wait(500);
	set_val(PS4_CIRCLE, 100);
	wait(100);
	wait(1500);
	set_val(PS4_CROSS, 100);
	wait(100);
	wait(500);
	combo_stop(RELEASE);
	colorled(0,0,0,0);
	} //Once this combo has ended, the combo it was called from can resume
combo TURBO {
	set_val(PS4_CIRCLE, 100);
	wait(300);
	wait(100);
	}
combo OLED_DISPLAY {
	// clear OLED screen
	cls_oled(0);
	colorled(1,0,0,0);
	print(0, 16, 1, 1, OLED_DISPLAY_0[0]);
	print(0, 40, 0, 1, OLED_DISPLAY_1[0]);
	wait(1200);
	cls_oled(0);
	// clear OLED screen
	cls_oled(0);
	colorled(1,1,0,0);
	print(0, 10, 1, 1, OLED_DISPLAY_2[0]);
	print(0, 30, 0, 1, OLED_DISPLAY_3[0]);
	print(0, 45, 0, 1, OLED_DISPLAY_4[0]);
	wait(1200);
	cls_oled(0);
	// clear OLED screen
	cls_oled(0);
	colorled(0,1,0,0);
	print(0, 10, 0, 1, OLED_DISPLAY_5[0]);
	print(0, 30, 0, 1, OLED_DISPLAY_6[0]);
	print(0, 50, 0, 1, OLED_DISPLAY_7[0]);
	wait(1200);
	cls_oled(0);
	// clear OLED screen
	combo_stop(OLED_DISPLAY);
	}
combo blinkON {
	colorled(0,0,1,0);
	wait(200);
	colorled(0,0,0,0);
	wait(200);
	colorled(0,0,1,0);
	wait(200);
	colorled(0,0,0,0);
	wait(200);
	combo_stop(blinkON);
	}
function colorled(a,b,c,d) {
	set_led(LED_1,a);
	set_led(LED_2,b);
	set_led(LED_3,c);
	set_led(LED_4,d);
	}
function center_x(f_chars,f_font) {
	return (OLED_WIDTH / 2) - ((f_chars * f_font) / 2);
	}
